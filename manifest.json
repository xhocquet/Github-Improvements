{
  "manifest_version": 2,
  "name": "GitHub Improvements",
  "version": "0.1",
  "author": "Xavier Hocquet",
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["jquery-3.1.1.slim.min.js","content.js"],
      "css": ["style.css"]
    }
  ]
}
